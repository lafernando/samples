import ballerina/grpc;

listener grpc:Listener ep = new (9090);

service StreamingCalcService on ep {

    resource function add(grpc:Caller caller, 
                          stream<int,error> clientStream) returns error? {
        check caller->send(10);
        check caller->complete();
    }

    resource function incrementalAdd(grpc:Caller caller, 
                                     stream<int,error> clientStream) returns error? {
        check caller->send(10);
        check caller->complete();
    }

}



const string ROOT_DESCRIPTOR = "";
function getDescriptorMap() returns map<string> {
    return {
        "streaming_calc.proto":"",
        "google/protobuf/wrappers.proto":""
        
    };
}

